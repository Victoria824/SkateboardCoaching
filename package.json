    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "replicate": "^0.25.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "snowboarding",
    "coaching",
    "ai",
    "video-analysis"
